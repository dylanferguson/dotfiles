{
  "window.zoomLevel": 1,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Blueberry dark theme",
  "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.associations": {
    "*.workflow": "yaml"
  },
  "editor.tabCompletion": "on",
  "window.restoreWindows": "all",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.iconTheme": "vscode-icons",
  "svelte.language-server.runtime": "/usr/local/bin/node",
  "window.restoreFullscreen": true,
  "files.autoSave": "afterDelay",
  "spellright.language": [
    "en-GB"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "Fira Code, JetBrains Mono, Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.renderWhitespace": "all",
  "update.showReleaseNotes": false,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.formatOnSave": true,
  "explorer.confirmDelete": false
}
